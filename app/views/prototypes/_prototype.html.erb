<div class="card">
  <%= link_to image_tag(prototype.image.variant(resize: '500!x500!'), class: :card__img ), prototype_path(prototype.id)%>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.name}", root_path, class: :card__user %>
  </div>
</div>
